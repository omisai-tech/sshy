{
    "servers": {
        "production-web": {
            "key": "~/.ssh/production_key"
        },
        "staging-web": {
            "key": "~/.ssh/staging_key",
            "user": "myuser"
        },
        "database-primary": {
            "key": "~/.ssh/db_key",
            "options": {
                "ProxyJump": "jump-host"
            }
        }
    },
    "private": [
        {
            "name": "personal-dev",
            "host": "dev.myserver.com",
            "user": "myuser",
            "port": 22,
            "key": "~/.ssh/personal_key",
            "tags": [
                "personal",
                "development"
            ]
        },
        {
            "name": "home-nas",
            "host": "192.168.1.100",
            "user": "admin",
            "port": 22,
            "key": "~/.ssh/nas_key",
            "tags": [
                "home",
                "storage"
            ]
        }
    ]
}